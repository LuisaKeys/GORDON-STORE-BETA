@model IEnumerable<MVC.Project.OnlineFurnitureSystem.Models.ViewModels.Pages.PageVM>

@{
    string tempPageTitle = (string)TempData["PageTitle"];
}

@foreach (var item in Model)
{
    if (tempPageTitle == item.Title)
    {
        <li><a class="active" href="/@item.Slug">@item.Title</a></li>
    }
    else
    {
        <li><a href="/@item.Slug">@item.Title</a></li>
    }
}

