@model Modelo.Sistema.UsuarioAdm
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Visualizando detalhes de um USUÁRIO";
}
<main>
<div class="tabelainteira">
    <div class="headertabela">
        VISUALIZANDO DETALHES DO USUÁRIO
    </div>
    <div class="panel-body">
        <div class="form-group">
            @Html.LabelFor(model => model.Id)<br />
            <div class="input-group">
                <span class="input-group-addon">
                    <i class="glyphicon glyphicon-user"></i>
                </span>
                @Html.EditorFor(model => model.Id, new
                {
                    htmlAttributes =
                new { @class = "form-control", disabled = "disabled" }
                })

            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.UserName)<br />
            <div class="input-group">
                <span class="input-group-addon">
                    <i class="glyphicon glyphicon-user"></i>
                </span>
                @Html.EditorFor(model => model.UserName, new
                {
                    htmlAttributes =
                new { @class = "form-control", disabled = "disabled" }
                })

            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.Email)<br />
            <div class="input-group">
                <span class="input-group-addon">
                    <i class="glyphicon glyphicon-user"></i>
                </span>
                @Html.EditorFor(model => model.Email, new
                {
                    htmlAttributes =
                new { @class = "form-control", disabled = "disabled" }
                })

            </div>
        </div>

    </div>
    <div class="panel-footer">
        @Html.ActionLink("Alterar", "Edit", null,
         new { @class = "btn btn-retornar" })
        @Html.ActionLink("Retornar para a listagem", "Index", null,
        new { @class = "btn btn-retornar" })
    </div>
</div>
</main>

